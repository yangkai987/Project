<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
>
    <TextView
            android:id="@+id/tv_chane_boy"
            android:text="男"
            android:textSize="@dimen/sp_16"
            android:gravity="center_horizontal"
            android:paddingTop="@dimen/dip20"
            android:paddingBottom="@dimen/dip20"
            android:textColor="@color/colorMain"
            android:background="@color/color_FFFFFF"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    <View style="@style/line_h_1"/>
    <TextView
            android:id="@+id/tv_chane_gird"
            android:text="女"
            android:textSize="@dimen/sp_16"
            android:gravity="center_horizontal"
            android:paddingTop="@dimen/dip20"
            android:paddingBottom="@dimen/dip20"
            android:textColor="@color/colorMain"
            android:background="@color/color_FFFFFF"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    <View style="@style/line_h_1"/>
    <TextView
            android:id="@+id/tv_chane_other"
            android:text="其他"
            android:textSize="@dimen/sp_16"
            android:gravity="center_horizontal"
            android:paddingTop="@dimen/dip20"
            android:paddingBottom="@dimen/dip20"
            android:textColor="@color/colorMain"
            android:background="@color/color_FFFFFF"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    <View style="@style/line_h_1"/>
    <TextView
            android:text="取消"
            android:id="@+id/tv_cancel"
            android:textSize="@dimen/sp_16"
            android:gravity="center_horizontal"
            android:paddingTop="@dimen/dip25"
            android:paddingBottom="@dimen/dip25"
            android:textColor="@color/color_FFFFFF"
            android:background="@color/color_4C545B"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    <View style="@style/line_h_1"/>
</LinearLayout>
