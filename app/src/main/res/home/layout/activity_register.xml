<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/rl_register_layout"
                android:orientation="vertical">

    <com.membership_score.widgetlib.widget.TitleBar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:title_show_backicon="true"
            app:title_text="注册" />
    <View   style="@style/line_h_1"
            android:id="@+id/view_1"/>
        <RelativeLayout
                android:paddingLeft="@dimen/dip50"
                android:paddingRight="@dimen/dip50"
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dip100">
            <TextView
                    android:id="@+id/tv_1"
                    android:text="昵称"
                    android:layout_centerVertical="true"
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/color_4C545B"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            <com.membership_score.widgetlib.widget.ClearEditText
                    android:id="@+id/et_login_member_nickname"
                    android:background="@null"
                    android:layout_alignParentRight="true"
                    android:textSize="@dimen/sp_16"
                    android:inputType="text"
                    android:drawablePadding="@dimen/dip15"
                    android:gravity="center|right"
                    android:layout_marginLeft="@dimen/dip15"
                    android:layout_toRightOf="@+id/tv_1"
                    android:textColor="@color/colorMain"
                    android:textColorHint="@color/color_CCCCCC"
                    android:hint="请输入会员昵称"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
        </RelativeLayout>
        <View   style="@style/line_h_1"/>
        <RelativeLayout
                android:paddingLeft="@dimen/dip50"
                android:paddingRight="@dimen/dip50"
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dip100">
            <TextView
                    android:id="@+id/tv_2"
                    android:text="手机号"
                    android:layout_centerVertical="true"
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/color_4C545B"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            <com.membership_score.widgetlib.widget.ClearEditText
                    android:id="@+id/et_member_phone_num"
                    android:background="@null"
                    android:drawablePadding="@dimen/dip15"
                    android:layout_alignParentRight="true"
                    android:textSize="@dimen/sp_16"
                    android:maxLength="11"
                    android:inputType="phone"
                    android:gravity="center|right"
                    android:layout_marginLeft="@dimen/dip20"
                    android:layout_toRightOf="@+id/tv_2"
                    android:textColor="@color/colorMain"
                    android:textColorHint="@color/color_CCCCCC"
                    android:hint="请输入会员手机号"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
        </RelativeLayout>
        <View
                style="@style/line_h_1"/>
        <RelativeLayout
                android:paddingLeft="@dimen/dip50"
                android:paddingRight="@dimen/dip50"
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dip100">
            <TextView
                    android:id="@+id/tv_3"
                    android:text="性别"
                    android:layout_centerVertical="true"
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/color_4C545B"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            <TextView
                    android:id="@+id/tv_member_sex"
                    android:background="@null"
                    android:drawablePadding="@dimen/dip15"
                    android:layout_alignParentRight="true"
                    android:textSize="@dimen/sp_16"
                    android:maxLength="11"
                    android:inputType="phone"
                    android:gravity="center|right"
                    android:layout_marginLeft="@dimen/dip20"
                    android:layout_toRightOf="@+id/tv_3"
                    android:textColor="@color/colorMain"
                    android:textColorHint="@color/color_CCCCCC"
                    android:hint="请选择会员性别"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
        </RelativeLayout>
        <View
                style="@style/line_h_1"/>
    <RelativeLayout
            android:paddingLeft="@dimen/dip50"
            android:paddingRight="@dimen/dip50"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dip100">
        <TextView
                android:id="@+id/tv_4"
                android:text="用户名"
                android:layout_centerVertical="true"
                android:textSize="@dimen/sp_16"
                android:textColor="@color/color_E84D61"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        <com.membership_score.widgetlib.widget.ClearEditText
                android:id="@+id/et_loginname_member"
                android:background="@null"
                android:layout_alignParentRight="true"
                android:textSize="@dimen/sp_16"
                android:inputType="text"
                android:drawablePadding="@dimen/dip15"
                android:gravity="center|right"
                android:layout_marginLeft="@dimen/dip15"
                android:layout_toRightOf="@+id/tv_4"
                android:textColor="@color/color_E84D61"
                android:textColorHint="@color/color_CCCCCC"
                android:hint="请输入会员登陆用户名"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
    </RelativeLayout>
        <View
                style="@style/line_h_1"/>
    <RelativeLayout
            android:paddingLeft="@dimen/dip50"
            android:paddingRight="@dimen/dip50"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dip100">
        <TextView
                android:id="@+id/tv_5"
                android:text="密码"
                android:layout_centerVertical="true"
                android:textSize="@dimen/sp_16"
                android:textColor="@color/color_E84D61"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        <com.membership_score.widgetlib.widget.ClearEditText
                android:id="@+id/et_login_pw_member"
                android:background="@null"
                android:layout_alignParentRight="true"
                android:textSize="@dimen/sp_16"
                android:inputType="text|textPassword"
                android:drawablePadding="@dimen/dip15"
                android:gravity="center|right"
                android:layout_marginLeft="@dimen/dip15"
                android:layout_toRightOf="@+id/tv_5"
                android:textColor="@color/color_E84D61"
                android:textColorHint="@color/color_CCCCCC"
                android:hint="请输入会员登陆密码"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
    </RelativeLayout>
        <View
                style="@style/line_h_1"/>
    <RelativeLayout
            android:paddingLeft="@dimen/dip50"
            android:paddingRight="@dimen/dip50"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dip100">
        <TextView
                android:id="@+id/tv_6"
                android:text="确认密码"
                android:layout_centerVertical="true"
                android:textSize="@dimen/sp_16"
                android:textColor="@color/color_E84D61"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        <com.membership_score.widgetlib.widget.ClearEditText
                android:id="@+id/et_login_pw_member_two"
                android:background="@null"
                android:layout_alignParentRight="true"
                android:textSize="@dimen/sp_16"
                android:inputType="text|textPassword"
                android:drawablePadding="@dimen/dip15"
                android:gravity="center|right"
                android:layout_marginLeft="@dimen/dip15"
                android:layout_toRightOf="@+id/tv_6"
                android:textColor="@color/color_E84D61"
                android:textColorHint="@color/color_CCCCCC"
                android:hint="请输入会员登陆确认密码"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
    </RelativeLayout>
        <View
                style="@style/line_h_1"/>
    <RelativeLayout
            android:paddingLeft="@dimen/dip50"
            android:paddingRight="@dimen/dip50"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dip100">
        <TextView
                android:id="@+id/tv_7"
                android:text="会员等级"
                android:layout_centerVertical="true"
                android:textSize="@dimen/sp_16"
                android:textColor="@color/color_E84D61"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        <com.membership_score.widgetlib.widget.ClearEditText
                android:id="@+id/et_login_pw_member_level"
                android:background="@null"
                android:layout_alignParentRight="true"
                android:textSize="@dimen/sp_16"
                android:drawablePadding="@dimen/dip15"
                android:gravity="center|right"
                android:inputType="number"
                android:layout_marginLeft="@dimen/dip15"
                android:layout_toRightOf="@+id/tv_7"
                android:textColor="@color/color_E84D61"
                android:textColorHint="@color/color_CCCCCC"
                android:hint="请输入会员等级"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
    </RelativeLayout>
        <View
                style="@style/line_h_1"/>

    <TextView
            android:id="@+id/tv_register"
            android:layout_below="@+id/ll_layout_1"
            android:layout_width="match_parent"
            android:text="注册"
            android:gravity="center"
            android:textSize="@dimen/sp_16"
            android:layout_margin="@dimen/dip100"
            android:background="@drawable/btn_login_bg_selector"
            android:textColor="@color/color_FFFFFF"
            android:layout_height="@dimen/dip100"/>
</LinearLayout>














